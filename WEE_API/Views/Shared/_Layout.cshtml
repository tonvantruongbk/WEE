    <script>
        $(document).ready(function () {
            var url = window.location;
            var menu = $('.page-sidebar-menu li a[href="' + url.pathname.replace('/Index', '') + '"]');
            menu.parent().addClass('active').addClass('open');
            menu.parent().parent().parent().addClass('active').addClass('open');
            menu.parent().parent().parent().children().find('span.arrow').addClass('open');
        });


        var fixedHeaderOffset = 0;
        if (App.getViewPort().width < App.getResponsiveBreakpoint('md')) {
            if ($('.page-header').hasClass('page-header-fixed-mobile')) {
                fixedHeaderOffset = $('.page-header').outerHeight(true);
            }
        } else if ($('body').hasClass('page-header-menu-fixed')) { // admin 3 fixed header menu mode
            fixedHeaderOffset = $('.page-header-menu').outerHeight(true);
        } else if ($('body').hasClass('page-header-top-fixed')) { // admin 3 fixed header top mode
            fixedHeaderOffset = $('.page-header-top').outerHeight(true);
        } else if ($('.page-header').hasClass('navbar-fixed-top')) {
            fixedHeaderOffset = $('.page-header').outerHeight(true);
        } else if ($('body').hasClass('page-header-fixed')) {
            fixedHeaderOffset = 64; // admin 5 fixed height
        }
        $.fn.dataTable.moment('DD/MM/YYYY');

        $.fn.dataTable.moment = function ( format, locale ) {
            var types = $.fn.dataTable.ext.type;

            // Add type detection
            types.detect.unshift( function (d) {
                return moment(d, format, locale, true ).isValid() ?
                    'moment-'+format :
                    null;
            } );
            // Add sorting method - use an integer for the sorting
            types.order[ 'moment-'+format+'-pre' ] = function ( d ) {
                return moment( d, format, locale, true ).unix();
            };
        };

        $.extend(true, $.fn.dataTable.defaults, {
            mark:true,
            //"dom": "<'row'<'col-md-8 col-sm-12'pli><'col-md-4 col-sm-12'<'table-group-actions pull-right'>>r><'table-responsive't><'row'<'col-md-8 col-sm-12'pli><'col-md-4 col-sm-12'>>", // datatable layout
            stripeClasses: [],
            searching: true,
            ordering: true,
            orderCellsTop: true,
            autoWidth: false,
            retrieve: false,
            responsive: true,
            processing: true,
            serverSide: true,
            //pagingType: 'bootstrap_extended',
            pageLength: 15,
            language: {
                url: '@Url.Content("~/Content/language.json")'
            },
            lengthMenu: [
                [5, 10, 15, 20, 30, 50, 100], [5, 10, 15, 20, 30, 50, 100]
            ], 
            fixedHeader: {
                header: true,
                headerOffset: fixedHeaderOffset
            }
        });
         
        $.fn.select2.defaults.set("width", "100%");

        var path = "@(Url.Action("Index", "Home"))";
    </script>

