@model WEE_API.ViewModel.DashBoardViewModel
@section PageStyles
{
    <!-- BEGIN PAGE LEVEL PLUGINS --> 
    <link href="@Url.Content("~/Content/assets/global/plugins/morris/morris.css")" rel="stylesheet" type="text/css" />
    <!-- END PAGE LEVEL PLUGINS -->
}
@section scripts{
    <!-- BEGIN PAGE LEVEL PLUGINS -->
    <script src="@Url.Content("~/Content/assets/global/plugins/morris/morris.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Content/assets/global/plugins/morris/raphael-min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Content/assets/global/plugins/counterup/jquery.waypoints.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Content/assets/global/plugins/counterup/jquery.counterup.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Content/assets/global/plugins/amcharts/amcharts/amcharts.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Content/assets/global/plugins/amcharts/amcharts/serial.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Content/assets/global/plugins/amcharts/amcharts/pie.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Content/assets/global/plugins/amcharts/amcharts/radar.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Content/assets/global/plugins/amcharts/amcharts/themes/light.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Content/assets/global/plugins/amcharts/amcharts/themes/patterns.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Content/assets/global/plugins/amcharts/amcharts/themes/chalk.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Content/assets/global/plugins/amcharts/amstockcharts/amstock.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Content/assets/global/plugins/horizontal-timeline/horizontal-timeline.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Content/assets/global/plugins/flot/jquery.flot.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Content/assets/global/plugins/flot/jquery.flot.resize.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Content/assets/global/plugins/flot/jquery.flot.categories.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Content/assets/global/plugins/jquery.sparkline.min.js")" type="text/javascript"></script>
    <!-- END PAGE LEVEL PLUGINS -->
    <script src="@Url.Content("~/Content/assets/pages/scripts/dashboard.min.js")" type="text/javascript"></script>
}

<!-- BEGIN PAGE HEADER-->
<!-- BEGIN PAGE TITLE-->
<h1 class="page-title">
    BẢNG TỔNG KẾT
    <small>Tổng hợp các thông tin cơ bản về WEE hiện tại</small>
</h1>
<!-- END PAGE TITLE-->
<!-- END PAGE HEADER-->
<div class="row">
    <div class="col-lg-3 col-md-3 col-sm-6 col-xs-12">
        <div class="dashboard-stat2 ">
            <div class="display">
                <div class="number">
                    <h3 class="font-green-sharp">
                        <span data-counter="counterup" data-value="@Model.TotalNumberInfo.CompanyNum"></span>
                    </h3>
                    <small>công ty</small>
                </div>
                <div class="icon">
                    <i class="icon-globe"></i>
                </div>
            </div>
            <div class="progress-info">
                <div class="progress">
                    <span style="width: @(Model.TotalNumberInfo.CompanyPer)%;" class="progress-bar progress-bar-success green-sharp">
                        <span class="sr-only">@(Model.TotalNumberInfo.CompanyPer)% được bình chọn</span>
                    </span>
                </div>
                <div class="status">
                    <div class="status-title"> được bình chọn </div>
                    <div class="status-number"> @(Model.TotalNumberInfo.CompanyPer)% </div>
                </div>
            </div>
        </div>
    </div>
    <div class="col-lg-3 col-md-3 col-sm-6 col-xs-12">
        <div class="dashboard-stat2 ">
            <div class="display">
                <div class="number">
                    <h3 class="font-purple-soft">
                        <span data-counter="counterup" data-value="@(Model.TotalNumberInfo.UserNum)"></span>
                    </h3>
                    <small>Người dùng</small>
                </div>
                <div class="icon">
                    <i class="icon-user"></i>
                </div>
            </div>
            <div class="progress-info">
                <div class="progress">
                    <span style="width: @(Model.TotalNumberInfo.CompanyPer)%;" class="progress-bar progress-bar-success purple-soft">
                        <span class="sr-only">97% đã bình chọn</span>
                    </span>
                </div>
                <div class="status">
                    <div class="status-title"> đã bình chọn </div>
                    <div class="status-number"> 97% </div>
                </div>
            </div>
        </div>
    </div>
    <div class="col-lg-3 col-md-3 col-sm-6 col-xs-12">
        <div class="dashboard-stat2 ">
            <div class="display">
                <div class="number">
                    <h3 class="font-blue-sharp">
                        <span data-counter="counterup" data-value="@(Model.TotalNumberInfo.JobNum)"></span>
                    </h3>
                    <small>Việc làm</small>
                </div>
                <div class="icon">
                    <i class="icon-folder"></i>
                </div>
            </div>
            <div class="progress-info">
                <div class="progress">
                    <span style="width: @(Model.TotalNumberInfo.CompanyPer)%;" class="progress-bar progress-bar-success blue-sharp">
                        <span class="sr-only">@(Model.TotalNumberInfo.CompanyPer)% tăng trưởng</span>
                    </span>
                </div>
                <div class="status">
                    <div class="status-title"> tăng trưởng </div>
                    <div class="status-number"> @(Model.TotalNumberInfo.CompanyPer)% </div>
                </div>
            </div>
        </div>
    </div>
    <div class="col-lg-3 col-md-3 col-sm-6 col-xs-12">
        <div class="dashboard-stat2 ">
            <div class="display">
                <div class="number">
                    <h3 class="font-red-haze">
                        <span data-counter="counterup" data-value="@(Model.TotalNumberInfo.RateNum)"></span>
                    </h3>
                    <small>Lượt bình chọn</small>
                </div>
                <div class="icon">
                    <i class="icon-like"></i>
                </div>
            </div>
            <div class="progress-info">
                <div class="progress">
                    <span style="width: @(Model.TotalNumberInfo.CompanyPer)%;" class="progress-bar progress-bar-success red-haze">
                        <span class="sr-only">@(Model.TotalNumberInfo.CompanyPer)% bình chọn</span>
                    </span>
                </div>
                <div class="status">
                    <div class="status-title"> bình chọn </div>
                    <div class="status-number"> @(Model.TotalNumberInfo.CompanyPer)% </div>
                </div>
            </div>
        </div>
    </div>
</div>
<div class="row">
    <div class="col-lg-12 col-xs-12 col-sm-12">
        <div class="portlet light ">
            <div class="portlet-title">
                <div class="caption">
                    <span class="caption-subject bold uppercase font-dark">Biểu đồ % bình chọn</span>
                </div>
            </div>
            <div class="portlet-body">
                <div id="dashboard_amchart_1" class="CSSAnimationChart"></div>
            </div>
        </div>
    </div>
    </div>
<div class="row">
    <div class="col-lg-12 col-xs-12 col-sm-12">
        <div class="portlet light ">
            <div class="portlet-title">
                <div class="caption ">
                    <span class="caption-subject font-dark bold uppercase">Số người bình chọn</span>
                </div>
            </div>
            <div class="portlet-body">
                <div id="dashboard_amchart_3" class="CSSAnimationChart"></div>
            </div>
        </div>
    </div>
</div> 
